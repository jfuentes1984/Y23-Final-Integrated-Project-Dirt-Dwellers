<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Dirt Dwellers</title>
    <link rel="stylesheet" href="~/W21_Assignment.styles.css" asp-append-version="true" />
</head>
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />


<body class="mainbody">
    <header>
        <nav class="navbar">

            <div class="menu">
                @* <a class="navbar-brand" asp-area="" asp-page="/Index">.NET Coffe</a> *@
                @* <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target=".navbar-collapse"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                    </button> *@
                <ul class="navbar-ul">
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-page="/About/Index">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-page="/Products/Index">Products</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-page="/Plants/Index">Plants</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-page="/Contact/Index">Contact</a>
                    </li>
                </ul>
                <partial name="_LoginPartial" />
            </div>
        </nav>
    </header>
    <div class="container">

        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="footer-container"><br>
        <div class="footer-social">LET'S GET SOCIAL</div>
        <ul class="footer-nav">
            <li>
                <a href="https://www.facebook.com" target="_blank">
                    <img class="footer-page-image" src="~/imgs/fbicon.png">
                </a>
            </li>
            <li>
                <a href="https://www.twitter.com" target="_blank">
                    <img class="footer-page-image" src="~/imgs/twittericon.png">
                </a>
            </li>
            <li>
                <a href="https://www.instagram.com" target="_blank">
                    <img class="footer-page-image" src="~/imgs/instagramicon.png">
                </a>
            </li>
            <li>
                <a href="https://www.pinterest.com" target="_blank">
                    <img class="footer-page-image" src="~/imgs/pinteresticon.png">
                </a>
            </li>
            <li>
                <a href="https://www.youtube.com" target="_blank">
                    <img class="footer-page-image" src="~/imgs/youtubeicon.jpg">
                </a>
            </li>
            <li>
                <a href="https://www.github.com" target="_blank">
                    <img class="footer-page-image" src="~/imgs/githubicon.jpg">
                </a>
            </li>
        </ul>

        <div class="footer-anochors"> <a asp-area="" asp-page="/Privacy"><br> Privacy
                Policy</a> | <a asp-area="" asp-page="/Terms">Terms of Use</a> | <a asp-area=""
                asp-page="~/Pages/Contact">Contact Us</a></div><br>
        <div class="footer">
            &copy; 2022 - Dirt Dwellers Inc., 123 Muddy St. J92 4N3, Québec
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>


</html>
